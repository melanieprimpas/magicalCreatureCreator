{
  "name": "magical-creature-creator",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "start": "concurrently \"npm run start-db\" \"npm run start:client\" ",
    "start-db": "cd server && npm run start",
    "start:client": "cd client && npm run build && npm run dev",
